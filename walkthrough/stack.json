{
	"AWSTemplateFormatVersion": "2010-09-09",
	"Description": "AWS MetricFilter",
	"Resources": {
		"LogGroup": {
			"Type": "AWS::Logs::LogGroup"
		},
		"Filter": {
			"Type": "AWS::Logs::MetricFilter",
			"Properties": {
				"MetricTransformations": [
					{
						"DefaultValue": 1,
						"MetricName": "404Count",
						"MetricValue": "1",
						"MetricNamespace": "WebServer/404s"
					}
				],
				"FilterPattern": "ERROR",
				"FilterName": "MyFilter",
				"LogGroupName": {
					"Ref": "LogGroup"
				}
			}
		}
	}
}